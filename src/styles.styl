@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@100;500;700&display=swap')

body {
  padding 0
  margin 0
  box-sizing border-box
  font-family Roboto, 'sans-serif'
}

.is-flex {
  display flex
}

.flex-center {
  align-items center
  justify-content center
}

.container {
  width 80%
  max-width 1200px
  flex-wrap wrap
  padding 0 10%
  margin 50px auto 0 auto
  text-align center
}

.text-center {
  text-align center
}
